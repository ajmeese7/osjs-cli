{
  "name": "___NAME___",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run test && rm ./dist/* && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@osjs/eslint-config": "^1.0.2",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "eslint": "^6.8.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "osjs": {
    "type": "package"
  }
}
